# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
	name = pwxu
	email = pwxu@coremail.cn
[alias]
	pick = cherry-pick
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%ai) %C(bold blue)<%an>%Creset' --abbrev-commit
        up = pull --rebase
        co = checkout
	squash-all = "!f(){ git reset $(git commit-tree HEAD^{tree} -m \"${1:-A new start}\");};f"
	desc = describe
        short-diff = diff --shortstat
        raw-diff = diff --raw
        fe = fetch --prune
        stat-conflicts = !git ls-files --unmerged
        apply-commit = am --ignore-space-change -3
[rebase]
#	autoSquash=true
	autoStash=true
[winUpdater]
	recentlySeenVersion = 2.20.1.windows.1
[core]
	autocrlf = false
	editor = vim
[difftool "sourcetree"]
	cmd = '' \"$LOCAL\" \"$REMOTE\"
[mergetool "sourcetree"]
	cmd = "'' "
	trustExitCode = true
[http]
	proxy = socks5://127.0.0.1:1080
        
